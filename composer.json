{
    "name": "binsoul/net-http-message-bridge",
    "description": "Implementation of PSR-7 URI and stream interfaces",
    "keywords": [
        "http",
        "message",
        "psr7"
    ],
    "homepage": "https://github.com/binsoul/net-http-message-bridge",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastian Mößler",
            "email": "code@binsoul.de",
            "homepage": "https://github.com/binsoul",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "psr/http-message": "~1.0",
        "binsoul/io-stream": "dev-master",
        "binsoul/net": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0",
        "fabpot/php-cs-fixer": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "BinSoul\\Bridge\\Http\\Message\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BinSoul\\Test\\Bridge\\Http\\Message\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "fix-src": "php-cs-fixer fix -v src",
        "fix-tests": "php-cs-fixer fix -v tests"
    }
}
